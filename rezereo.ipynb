    "import random\n",
    "import time\n",
    "i = 0\n",
    "\n",
    "# 各アイコンのリスト\n",
    "icons = [\"青\", \"黄色\", \"緑\", \"赤\", \"鬼\"]\n",
    "\n",
    "# 各アイコンの獲得確率（パーセント）\n",
    "probabilities = [40, 30, 15, 10, 5]\n",
    "\n",
    "# 重み付きランダム選択を行う\n",
    "selected_icon = random.choices(icons, weights=probabilities, k=1)[0]\n",
    "\n",
    "print(f\"獲得したアイコン: {selected_icon}\")\n",
    "\n",
    "\n",
    "       "
   ]
  }
